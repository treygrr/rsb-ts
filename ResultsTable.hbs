<!doctype html>
<html>
<body>
  <div>
    <div>
      <table class="styled-table">
        <thead>
          <tr>
            <th>Image</th>
            <th>Name</th>
            <th>Item ID</th>
            <th>Memebers</th>
            <th>Price</th>
            <th>Change</th>
          </tr>
        </thead>
        <tbody>
          {{#each data}}
          <tr>
            <td><img src="{{this.image}}"></td>
            <td>{{this.name}}</td>
            <td>{{this.id}}</td>
            <td>{{this.members}}</td>
            <td>{{this.price}}</td>
            <td>{{this.change}}</td>
          </tr>
          {{/each}}
        </tbody>
      </table>
      </div>
  </div>
</body>

</html>